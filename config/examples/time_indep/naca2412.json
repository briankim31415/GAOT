{
    "setup": {
        "seed": 42,
        "device": "cuda:0",
        "dtype": "torch.float32",
        "trainer_name": "static",
        "train": true,
        "test": false,
        "ckpt": false
    },
    "model": {
        "name": "goat",
        "latent_tokens_size": [
            64,
            64
        ],
        "args": {
            "magno": {
                "coord_dim": 2,
                "radius": 0.033,
                "hidden_size": 64,
                "mlp_layers": 3,
                "lifting_channels": 64
            },
            "transformer": {
                "patch_size": 2,
                "hidden_size": 256
            }
        }
    },
    "dataset": {
        "name": "NACA2412",
        "metaname": "compressible_flow/naca2412",
        "base_path": "datasets/time_indep/",
        "train_size": 16,
        "val_size": 4,
        "test_size": 4,
        "batch_size": 64,
        "num_workers": 4,
        "shuffle": true
    },
    "optimizer": {
        "name": "adamw",
        "args": {
            "lr": 0.0008,
            "weight_decay": 1e-05,
            "epoch": 1,
            "loss_scale": 1.0,
            "eval_every_eps": 2,
            "scheduler": "mix",
            "max_lr": 0.001,
            "min_lr": 0.0001,
            "final_lr": 5e-05
        }
    },
    "path": {
        "ckpt_path": ".ckpt/examples/time_indep/naca2412.pt",
        "loss_path": ".loss/examples/time_indep/naca2412.png",
        "result_path": ".results/examples/time_indep/naca2412.png",
        "database_path": ".database/examples/time_indep/naca2412.csv"
    }
}
